{
   "name": "ZXing.SkiaSharp",
   "version": "0.16.1",
   "dependencies": {
      "NETStandard.Library": "1.6.0",
      "SkiaSharp": "1.57.0"
   },
   "frameworks": {
      "netstandard1.3": {
         "dependencies": {
            "ZXing.Net": {
               "target": "project"
            }
         }
      }
   },
   "buildOptions": {
      "define": [ "NETSTANDARD" ],
      "optimize": true,
      "xmlDoc": true,
      "platform": "anycpu",
      "publicSign": false,
      "keyFile": "../../../Key/private.snk",
      "debugType": "portable",
      "outputName": "zxing.skiasharp",
      "allowUnsafe": true,
      "compile": {
         "includeFiles": [
            "Properties/AssemblyInfo.cs",
            "BarcodeReader.cs",
            "BarcodeReader.Extensions.cs",
            "SKBitmapLuminanceSource.cs"
         ]
      }
   }
}
