{
   "name": "ZXing.Net",
   "version": "0.15.0.0",
   "dependencies": {
      "NETStandard.Library": "1.6.0"
   },
   "frameworks": {
      "netstandard1.0": {},
      "netstandard1.1": {
         "buildOptions": {
            "compile": {
               "exclude": [
                  "../common/BigInteger"
               ]
            }
         }
      },
      "netstandard1.3": {
         "buildOptions": {
            "compile": {
               "exclude": [
                  "../common/BigInteger"
               ]
            }
         }
      }
   },
   "buildOptions": {
      "define": [ "NETSTANDARD" ],
      "optimize": true,
      "xmlDoc": true,
      "platform": "anycpu",
      "keyFile": "../../../Key/private.snk",
      "publicSign": true,
      "debugType": "portable",
      "outputName": "zxing",
      "compile": {
         "include": [
            "../aztec/**",
            "../client/**",
            "../common/**",
            "../datamatrix/**",
            "../imb/**",
            "../maxicode/**",
            "../multi/**",
            "../oned/**",
            "../pdf417/**",
            "../qrcode/**"
         ],
         "exclude": [
            "../client/result/optional"
         ],
         "includeFiles": [
            "../net2.0/Action.cs",
            "../net2.0/Func.cs",
            "../Properties/AssemblyInfo.cs",
            "../renderer/IBarcodeRenderer.cs",
            "../renderer/PixelData.cs",
            "../renderer/PixelDataRenderer.cs",
            "../renderer/SVGRenderer.cs",
            "../silverlight/BrowsableAttribute.cs",
            "../silverlight/NonSerializedAttribute.cs",
            "../silverlight/SerializableAttribute.cs",
            "../BarcodeFormat.cs",
            "../BarcodeReader.ByteArray.cs",
            "../BarcodeReaderGeneric.cs",
            "../BarcodeWriter.PixelData.cs",
            "../BarcodeWriter.SvgImage.cs",
            "../BarcodeWriterGeneric.cs",
            "../BaseLuminanceSource.cs",
            "../Binarizer.cs",
            "../BinaryBitmap.cs",
            "../DecodeHintType.cs",
            "../Dimension.cs",
            "../EncodeHintType.cs",
            "../FormatException.cs",
            "../IBarcodeReader.cs",
            "../IBarcodeReaderGeneric.cs",
            "../IBarcodeWriter.cs",
            "../IBarcodeWriterGeneric.cs",
            "../IMultipleBarcodeReader.cs",
            "../IMultipleBarcodeReaderGeneric.cs",
            "../InvertedLuminanceSource.cs",
            "../LuminanceSource.cs",
            "../MultiFormatReader.cs",
            "../MultiFormatWriter.cs",
            "../PlanarYUVLuminanceSource.cs",
            "../Reader.cs",
            "../ReaderException.cs",
            "../Result.cs",
            "../ResultMetadataType.cs",
            "../ResultPoint.cs",
            "../ResultPointCallback.cs",
            "../RGB565LuminanceSource.cs",
            "../RGBLuminanceSource.cs",
            "../SupportClass.cs",
            "../Writer.cs",
            "../WriterException.cs"
         ],
         "excludeFiles": [
            "../common/BitMatrix.Drawing.cs",
            "../common/BitMatrix.Silverlight.cs"
         ]
      }
   }
}
